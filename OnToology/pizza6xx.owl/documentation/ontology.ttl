@prefix : <http://www.semanticweb.org/bina/ontologies/2023/9/pizzaxxx#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://www.semanticweb.org/bina/ontologies/2023/9/pizzaxxx#> .

<http://www.semanticweb.org/bina/ontologies/2023/9/pizzaxxx> rdf:type owl:Ontology .

#################################################################
#    Annotation properties
#################################################################

###  http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled
<http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled> rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://www.semanticweb.org/bina/ontologies/2023/9/pizzaxxx#hasBase
:hasBase rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf :hasIngredient ;
         owl:inverseOf :isBaseOf ;
         rdf:type owl:FunctionalProperty ;
         rdfs:domain :Pizza ,
                     [ rdf:type owl:Restriction ;
                       owl:onProperty :hasBase ;
                       owl:someValuesFrom :PizzaBase
                     ] ,
                     [ rdf:type owl:Restriction ;
                       owl:onProperty :hasCalorificContentValue ;
                       owl:someValuesFrom xsd:integer
                     ] ;
         rdfs:range :PizzaBase .


###  http://www.semanticweb.org/bina/ontologies/2023/9/pizzaxxx#hasCountryOfOrigin
:hasCountryOfOrigin rdf:type owl:ObjectProperty .


###  http://www.semanticweb.org/bina/ontologies/2023/9/pizzaxxx#hasIngredient
:hasIngredient rdf:type owl:ObjectProperty ;
               owl:inverseOf :isIngredientOf ;
               rdf:type owl:TransitiveProperty .


###  http://www.semanticweb.org/bina/ontologies/2023/9/pizzaxxx#hasSpiciness
:hasSpiciness rdf:type owl:ObjectProperty ,
                       owl:FunctionalProperty ;
              rdfs:range :SpicinessValuePartition ,
                         :ValuePartition ,
                         [ rdf:type owl:Class ;
                           owl:unionOf ( :Hot
                                         :Medium
                                         :Mild
                                       )
                         ] .


###  http://www.semanticweb.org/bina/ontologies/2023/9/pizzaxxx#hasSpicinessPreference
:hasSpicinessPreference rdf:type owl:ObjectProperty ;
                        rdfs:domain :Customer ,
                                    :Person ,
                                    [ owl:intersectionOf ( :Person
                                                           [ rdf:type owl:Restriction ;
                                                             owl:onProperty :purchasedPizza ;
                                                             owl:someValuesFrom :Pizza
                                                           ]
                                                           [ rdf:type owl:Restriction ;
                                                             owl:onProperty :hasPhone ;
                                                             owl:someValuesFrom xsd:string
                                                           ]
                                                         ) ;
                                      rdf:type owl:Class
                                    ] ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty :purchasedPizza ;
                                      owl:someValuesFrom :Pizza
                                    ] ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty :hasPhone ;
                                      owl:someValuesFrom xsd:string
                                    ] ;
                        rdfs:range :SpicinessValuePartition ,
                                   :ValuePartition ,
                                   [ rdf:type owl:Class ;
                                     owl:unionOf ( :Hot
                                                   :Medium
                                                   :Mild
                                                 )
                                   ] .


###  http://www.semanticweb.org/bina/ontologies/2023/9/pizzaxxx#hasTopping
:hasTopping rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf :hasIngredient ;
            owl:inverseOf :isToppingOf ;
            rdfs:domain :Pizza ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :hasBase ;
                          owl:someValuesFrom :PizzaBase
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :hasCalorificContentValue ;
                          owl:someValuesFrom xsd:integer
                        ] ;
            rdfs:range :PizzaTopping .


###  http://www.semanticweb.org/bina/ontologies/2023/9/pizzaxxx#isBaseOf
:isBaseOf rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf :isIngredientOf ;
          rdf:type owl:InverseFunctionalProperty ;
          rdfs:domain :PizzaBase ;
          rdfs:range :Pizza ,
                     [ rdf:type owl:Restriction ;
                       owl:onProperty :hasBase ;
                       owl:someValuesFrom :PizzaBase
                     ] ,
                     [ rdf:type owl:Restriction ;
                       owl:onProperty :hasCalorificContentValue ;
                       owl:someValuesFrom xsd:integer
                     ] .


###  http://www.semanticweb.org/bina/ontologies/2023/9/pizzaxxx#isIngredientOf
:isIngredientOf rdf:type owl:ObjectProperty ,
                         owl:TransitiveProperty .


###  http://www.semanticweb.org/bina/ontologies/2023/9/pizzaxxx#isToppingOf
:isToppingOf rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf :isIngredientOf ;
             rdfs:domain :PizzaTopping ;
             rdfs:range :Pizza ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :hasBase ;
                          owl:someValuesFrom :PizzaBase
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :hasCalorificContentValue ;
                          owl:someValuesFrom xsd:integer
                        ] .


###  http://www.semanticweb.org/bina/ontologies/2023/9/pizzaxxx#purchasedByCustomer
:purchasedByCustomer rdf:type owl:ObjectProperty ;
                     owl:inverseOf :purchasedPizza ;
                     rdfs:domain :Pizza ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty :hasBase ;
                                   owl:someValuesFrom :PizzaBase
                                 ] ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty :hasCalorificContentValue ;
                                   owl:someValuesFrom xsd:integer
                                 ] ;
                     rdfs:range :Customer ,
                                :Person ,
                                [ owl:intersectionOf ( :Person
                                                       [ rdf:type owl:Restriction ;
                                                         owl:onProperty :purchasedPizza ;
                                                         owl:someValuesFrom :Pizza
                                                       ]
                                                       [ rdf:type owl:Restriction ;
                                                         owl:onProperty :hasPhone ;
                                                         owl:someValuesFrom xsd:string
                                                       ]
                                                     ) ;
                                  rdf:type owl:Class
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty :purchasedPizza ;
                                  owl:someValuesFrom :Pizza
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty :hasPhone ;
                                  owl:someValuesFrom xsd:string
                                ] .


###  http://www.semanticweb.org/bina/ontologies/2023/9/pizzaxxx#purchasedPizza
:purchasedPizza rdf:type owl:ObjectProperty ;
                rdfs:domain :Customer ,
                            :Person ,
                            [ owl:intersectionOf ( :Person
                                                   [ rdf:type owl:Restriction ;
                                                     owl:onProperty :purchasedPizza ;
                                                     owl:someValuesFrom :Pizza
                                                   ]
                                                   [ rdf:type owl:Restriction ;
                                                     owl:onProperty :hasPhone ;
                                                     owl:someValuesFrom xsd:string
                                                   ]
                                                 ) ;
                              rdf:type owl:Class
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :purchasedPizza ;
                              owl:someValuesFrom :Pizza
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :hasPhone ;
                              owl:someValuesFrom xsd:string
                            ] ;
                rdfs:range :Pizza ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :hasBase ;
                             owl:someValuesFrom :PizzaBase
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :hasCalorificContentValue ;
                             owl:someValuesFrom xsd:integer
                           ] .


#################################################################
#    Data properties
#################################################################

###  http://www.semanticweb.org/bina/ontologies/2023/9/pizzaxxx#hasCalorificContentValue
:hasCalorificContentValue rdf:type owl:DatatypeProperty ,
                                   owl:FunctionalProperty ;
                          rdfs:range xsd:integer .


###  http://www.semanticweb.org/bina/ontologies/2023/9/pizzaxxx#hasDiscount
:hasDiscount rdf:type owl:DatatypeProperty ;
             rdfs:domain :Customer ,
                         :Person ,
                         [ owl:intersectionOf ( :Person
                                                [ rdf:type owl:Restriction ;
                                                  owl:onProperty :purchasedPizza ;
                                                  owl:someValuesFrom :Pizza
                                                ]
                                                [ rdf:type owl:Restriction ;
                                                  owl:onProperty :hasPhone ;
                                                  owl:someValuesFrom xsd:string
                                                ]
                                              ) ;
                           rdf:type owl:Class
                         ] ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty :purchasedPizza ;
                           owl:someValuesFrom :Pizza
                         ] ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty :hasPhone ;
                           owl:someValuesFrom xsd:string
                         ] ;
             rdfs:range xsd:decimal .


###  http://www.semanticweb.org/bina/ontologies/2023/9/pizzaxxx#hasPhone
:hasPhone rdf:type owl:DatatypeProperty ;
          rdfs:domain :Person ;
          rdfs:range xsd:string .


###  http://www.semanticweb.org/bina/ontologies/2023/9/pizzaxxx#numberOfPizzasPurchased
:numberOfPizzasPurchased rdf:type owl:DatatypeProperty ;
                         rdfs:domain :Customer ,
                                     :Person ,
                                     [ owl:intersectionOf ( :Person
                                                            [ rdf:type owl:Restriction ;
                                                              owl:onProperty :purchasedPizza ;
                                                              owl:someValuesFrom :Pizza
                                                            ]
                                                            [ rdf:type owl:Restriction ;
                                                              owl:onProperty :hasPhone ;
                                                              owl:someValuesFrom xsd:string
                                                            ]
                                                          ) ;
                                       rdf:type owl:Class
                                     ] ,
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty :purchasedPizza ;
                                       owl:someValuesFrom :Pizza
                                     ] ,
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty :hasPhone ;
                                       owl:someValuesFrom xsd:string
                                     ] ;
                         rdfs:range xsd:integer .


###  http://www.semanticweb.org/bina/ontologies/2023/9/pizzaxxx#ssn
:ssn rdf:type owl:DatatypeProperty ;
     rdfs:domain :Employee ,
                 :Person ,
                 [ owl:intersectionOf ( :Person
                                        [ rdf:type owl:Restriction ;
                                          owl:onProperty :ssn ;
                                          owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                          owl:onDataRange xsd:string
                                        ]
                                      ) ;
                   rdf:type owl:Class
                 ] ,
                 [ rdf:type owl:Restriction ;
                   owl:onProperty :ssn ;
                   owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                   owl:onDataRange xsd:string
                 ] ;
     rdfs:range xsd:string .


#################################################################
#    Classes
#################################################################

###  http://www.semanticweb.org/bina/ontologies/2023/9/pizzaxxx#AmericanHotPizza
:AmericanHotPizza rdf:type owl:Class ;
                  rdfs:subClassOf :NamedPizza ,
                                  :Pizza ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty :hasBase ;
                                    owl:someValuesFrom :PizzaBase
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty :hasTopping ;
                                    owl:someValuesFrom :CheeseTopping
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty :hasTopping ;
                                    owl:someValuesFrom :JalapenoTopping
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty :hasTopping ;
                                    owl:someValuesFrom :MozzarellaTopping
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty :hasTopping ;
                                    owl:someValuesFrom :PepperoniTopping
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty :hasTopping ;
                                    owl:someValuesFrom :TomatoTopping
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty :hasTopping ;
                                    owl:allValuesFrom [ rdf:type owl:Class ;
                                                        owl:unionOf ( :JalapenoTopping
                                                                      :MozzarellaTopping
                                                                      :PepperoniTopping
                                                                      :TomatoTopping
                                                                    )
                                                      ]
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty :hasCalorificContentValue ;
                                    owl:someValuesFrom xsd:integer
                                  ] .


###  http://www.semanticweb.org/bina/ontologies/2023/9/pizzaxxx#AmericanPizza
:AmericanPizza rdf:type owl:Class ;
               rdfs:subClassOf :NamedPizza ,
                               :Pizza ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty :hasBase ;
                                 owl:someValuesFrom :PizzaBase
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty :hasTopping ;
                                 owl:someValuesFrom :CheeseTopping
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty :hasTopping ;
                                 owl:someValuesFrom :MozzarellaTopping
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty :hasTopping ;
                                 owl:someValuesFrom :PepperoniTopping
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty :hasTopping ;
                                 owl:someValuesFrom :TomatoTopping
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty :hasTopping ;
                                 owl:allValuesFrom [ rdf:type owl:Class ;
