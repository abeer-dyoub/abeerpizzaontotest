###  http://www.semanticweb.org/bina/ontologies/2023/9/pizzaxxx#AnchovyTopping
:AnchovyTopping rdf:type owl:Class ;
                rdfs:subClassOf :PizzaTopping ,
                                :SeaFoodTopping .


###  http://www.semanticweb.org/bina/ontologies/2023/9/pizzaxxx#CaperTopping
:CaperTopping rdf:type owl:Class ;
              rdfs:subClassOf :PizzaTopping ,
                              :VegetableTopping ,
                              [ rdf:type owl:Class ;
                                owl:unionOf ( :CheeseTopping
                                              :VegetableTopping
                                            )
                              ] .


###  http://www.semanticweb.org/bina/ontologies/2023/9/pizzaxxx#CheeseTopping
:CheeseTopping rdf:type owl:Class ;
               rdfs:subClassOf :PizzaTopping ,
                               [ rdf:type owl:Class ;
                                 owl:unionOf ( :CheeseTopping
                                               :VegetableTopping
                                             )
                               ] .


###  http://www.semanticweb.org/bina/ontologies/2023/9/pizzaxxx#CheesyPizza
:CheesyPizza rdf:type owl:Class ;
             owl:equivalentClass [ owl:intersectionOf ( :Pizza
                                                        [ rdf:type owl:Restriction ;
                                                          owl:onProperty :hasTopping ;
                                                          owl:someValuesFrom :CheeseTopping
                                                        ]
                                                      ) ;
                                   rdf:type owl:Class
                                 ] ;
             rdfs:subClassOf :Pizza ,
                             [ owl:intersectionOf ( :Pizza
                                                    [ rdf:type owl:Restriction ;
                                                      owl:onProperty :hasTopping ;
                                                      owl:someValuesFrom :CheeseTopping
                                                    ]
                                                  ) ;
                               rdf:type owl:Class
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :hasBase ;
                               owl:someValuesFrom :PizzaBase
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :hasTopping ;
                               owl:someValuesFrom :CheeseTopping
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :hasCalorificContentValue ;
                               owl:someValuesFrom xsd:integer
                             ] .


###  http://www.semanticweb.org/bina/ontologies/2023/9/pizzaxxx#Country
:Country rdf:type owl:Class ;
         owl:equivalentClass [ rdf:type owl:Class ;
                               owl:oneOf ( :America
                                           :England
                                           :France
                                           :Germany
                                           :Italy
                                         )
                             ] ;
         rdfs:subClassOf [ rdf:type owl:Class ;
                           owl:oneOf ( :America
                                       :England
                                       :France
                                       :Germany
                                       :Italy
                                     )
                         ] ;
         rdfs:comment """this class is a set of countries
it is an enumerated class""" ;
         rdfs:isDefinedBy <https://www.disim.univaq.it/AbeerDyoub> ;
         rdfs:label "Paese"@it ;
         owl:versionInfo "version1" .


###  http://www.semanticweb.org/bina/ontologies/2023/9/pizzaxxx#Customer
:Customer rdf:type owl:Class ;
          owl:equivalentClass [ owl:intersectionOf ( :Person
                                                     [ rdf:type owl:Restriction ;
                                                       owl:onProperty :purchasedPizza ;
                                                       owl:someValuesFrom :Pizza
                                                     ]
                                                     [ rdf:type owl:Restriction ;
                                                       owl:onProperty :hasPhone ;
                                                       owl:someValuesFrom xsd:string
                                                     ]
                                                   ) ;
                                rdf:type owl:Class
                              ] ;
          rdfs:subClassOf :Person ,
                          [ owl:intersectionOf ( :Person
                                                 [ rdf:type owl:Restriction ;
                                                   owl:onProperty :purchasedPizza ;
                                                   owl:someValuesFrom :Pizza
                                                 ]
                                                 [ rdf:type owl:Restriction ;
                                                   owl:onProperty :hasPhone ;
                                                   owl:someValuesFrom xsd:string
                                                 ]
                                               ) ;
                            rdf:type owl:Class
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :purchasedPizza ;
                            owl:someValuesFrom :Pizza
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :hasPhone ;
                            owl:someValuesFrom xsd:string
                          ] .


###  http://www.semanticweb.org/bina/ontologies/2023/9/pizzaxxx#DeepPan
:DeepPan rdf:type owl:Class ;
         rdfs:subClassOf :PizzaBase .


###  http://www.semanticweb.org/bina/ontologies/2023/9/pizzaxxx#Employee
:Employee rdf:type owl:Class ;
          rdfs:subClassOf :Person ,
                          [ owl:intersectionOf ( :Person
                                                 [ rdf:type owl:Restriction ;
                                                   owl:onProperty :ssn ;
                                                   owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                   owl:onDataRange xsd:string
                                                 ]
                                               ) ;
                            rdf:type owl:Class
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :ssn ;
                            owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                            owl:onDataRange xsd:string
                          ] .


###  http://www.semanticweb.org/bina/ontologies/2023/9/pizzaxxx#GreenPepperTopping
:GreenPepperTopping rdf:type owl:Class ;
                    rdfs:subClassOf :PepperTopping ,
                                    :PizzaTopping ,
                                    :VegetableTopping ,
                                    [ rdf:type owl:Class ;
                                      owl:unionOf ( :CheeseTopping
                                                    :VegetableTopping
                                                  )
                                    ] ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty :hasSpiciness ;
                                      owl:someValuesFrom :Medium
                                    ] .


###  http://www.semanticweb.org/bina/ontologies/2023/9/pizzaxxx#HamTopping
:HamTopping rdf:type owl:Class ;
            rdfs:subClassOf :MeatTopping ,
                            :PizzaTopping .


###  http://www.semanticweb.org/bina/ontologies/2023/9/pizzaxxx#HighCaloriePizza
:HighCaloriePizza rdf:type owl:Class ;
                  owl:equivalentClass [ owl:intersectionOf ( :Pizza
                                                             [ rdf:type owl:Restriction ;
                                                               owl:onProperty :hasCalorificContentValue ;
                                                               owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                                                    owl:onDatatype xsd:integer ;
                                                                                    owl:withRestrictions ( [ xsd:minInclusive 400
                                                                                                           ]
                                                                                                         )
                                                                                  ]
                                                             ]
                                                           ) ;
                                        rdf:type owl:Class
                                      ] ;
                  rdfs:subClassOf :Pizza ,
                                  [ owl:intersectionOf ( :Pizza
                                                         [ rdf:type owl:Restriction ;
                                                           owl:onProperty :hasCalorificContentValue ;
                                                           owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                                                owl:onDatatype xsd:integer ;
                                                                                owl:withRestrictions ( [ xsd:minInclusive 400
                                                                                                       ]
                                                                                                     )
                                                                              ]
                                                         ]
                                                       ) ;
                                    rdf:type owl:Class
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty :hasBase ;
                                    owl:someValuesFrom :PizzaBase
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty :hasCalorificContentValue ;
                                    owl:someValuesFrom xsd:integer
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty :hasCalorificContentValue ;
                                    owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                         owl:onDatatype xsd:integer ;
                                                         owl:withRestrictions ( [ xsd:minInclusive 400
                                                                                ]
                                                                              )
                                                       ]
                                  ] .


###  http://www.semanticweb.org/bina/ontologies/2023/9/pizzaxxx#Hot
:Hot rdf:type owl:Class ;
